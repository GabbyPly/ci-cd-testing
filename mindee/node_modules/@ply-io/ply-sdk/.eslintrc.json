{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"plugins": ["jest", "prettier"],
	"rules": {
		"prettier/prettier": ["error"],
		"no-prototype-builtins": "off"
	}
}
